    </ul>
   <ul class="nav navbar-nav">
      <li><a href="#">Link</a></li>
    </ul>
   <ul class="nav navbar-nav">
      <li><a href="#">Link</a></li>
    </ul>
  </div>
</div>

    <%= yield %>

    <!-- Javascripts
    ================================================== -->
